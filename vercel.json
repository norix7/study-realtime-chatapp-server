{
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "headers": [
    {
      "source": "/socket.io/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": ["https://study-realtime-chatapp-client.vercel.app", "http://localhost:3000"] }
      ]
    }
  ]
}
